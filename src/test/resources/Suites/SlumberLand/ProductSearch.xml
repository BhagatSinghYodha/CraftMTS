            <class name="testcases.SlumberLand.ProductSearch">
                <methods>
                    <include name="TC_0026_VerifyNoProductsFoundMsg"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="SearchProductsTests3">
        <classes>
            <class name="testcases.SlumberLand.ProductSearch">
                <methods>
                    <include name="TC_0083_VerifyGuestUserProdSearchByProductID"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="SearchProductsTests4">
        <classes>
            <class name="testcases.SlumberLand.ProductSearch">
                <methods>
                    <include name="TC_0084_VerifyRegUserProdSearchByProductID"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>
        <classes>
            <class name="testcases.SlumberLand.ProductSearch">
                <methods>
                    <include name="TC_0029_VerifyProductSearchFunctionality"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="SearchProductsTests2">
